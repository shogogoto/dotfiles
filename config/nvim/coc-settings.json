{
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "python": {
      "command": "ruff-lsp"
    }
  },
  "suggest.keepCompleteopt": true,
  "typescript.suggest.autoImports": true,
  "typescript.autoClosingTags": true,
  "explorer.root.customRules": {
    "vcs": {
      "patterns": [".git", ".hg", ".projections.json"]
    },
    "vcs-r": {
      "patterns": [".git", ".hg", ".projections.json"],
      "bottomUp": true
    }
  },
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "workspace.workspaceFolderCheckCwd": false,
  "snippets.ultisnips.pythonPrompt": false,
  "coc.preferences.formatOnSave": true,
  "ruff.enableExperimentalFormatter": true,
  "ruff.autoFixOnSave": true,
  "ruff.client.codeAction.showDocumantaion.enable": true,
  "python.analysis.diagnosticMode": "workspace",
  "coc.source.ultisnips.enable": false,
  "python.linting.enabled": false,
  "pyright.testing.provider": "pytest",
  "python.linting.ruffEnabled": true
}
